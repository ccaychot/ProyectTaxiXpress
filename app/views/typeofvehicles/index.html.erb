<p id="notice"><%= notice %></p>

<h1>Listing Typeofvehicles</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @typeofvehicles.each do |typeofvehicle| %>
      <tr>
        <td><%= typeofvehicle.description %></td>
        <td><%= link_to 'Show', typeofvehicle %></td>
        <td><%= link_to 'Edit', edit_typeofvehicle_path(typeofvehicle) %></td>
        <td><%= link_to 'Destroy', typeofvehicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Typeofvehicle', new_typeofvehicle_path %>
